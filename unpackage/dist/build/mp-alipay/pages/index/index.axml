<view class="container"><scroll-view class="timer-section" scroll-y="{{true}}"><view class="timer-box"><block a:if="{{timers.length>0}}"><view class="timers-list"><view class="timer-area" a:for="{{$root.l0}}" a:for-item="timer" a:for-index="index" a:key="index"><view class="timer-content"><image class="timer-icon" src="{{timer.$orig.icon}}" mode="aspectFit"></image><view class="timer-info"><text class="timer-name">{{timer.$orig.name}}</text><text class="timer-countdown">{{timer.m0}}</text></view></view><button data-event-opts="{{[['tap',[['stopTimer',[index]]]]]}}" class="close-btn" onTap="__e">×</button></view></view></block><block a:else><view class="empty-timer"><text>下点东西778~</text></view></block></view></scroll-view><view class="food-section"><scroll-view class="category-nav" scroll-x="{{true}}"><view data-event-opts="{{[['tap',[['changeCategory',[index]]]]]}}" class="{{(('category-item')+' '+(activeCategory===index?'active':''))}}" onTap="__e" a:for="{{categories}}" a:for-item="category" a:for-index="index" a:key="index">{{''+category+''}}</view><view data-event-opts="{{[['tap',[['addCategory',['$event']]]]]}}" class="add-category" onTap="__e">+ 添加</view></scroll-view><scroll-view class="food-list" scroll-y="{{true}}"><view data-event-opts="{{[['tap',[['startTimer',['$0'],[[['filteredItems','',index]]]]]]]}}" class="food-card" onTap="__e" a:for="{{filteredItems}}" a:for-item="item" a:for-index="index" a:key="index"><image class="food-icon" src="{{item.icon}}" mode="aspectFit"></image><text class="food-name">{{item.name}}</text><text class="food-time">{{item.time+" 秒"}}</text><button data-event-opts="{{[['tap',[['editTime',[index]]]]]}}" class="edit-btn" catchTap="__e">编辑</button></view></scroll-view><button data-event-opts="{{[['tap',[['addFood',['$event']]]]]}}" class="add-btn" onTap="__e">+ 添加食材</button></view><view class="bottom-nav"><view class="nav-item active">Timer</view><view data-event-opts="{{[['tap',[['goToSettings',['$event']]]]]}}" class="nav-item" onTap="__e">setting</view></view><uni-popup vue-id="8deb4120-1" type="dialog" ref="__r" data-ref="popup" onVueInit="__l" vue-slots="{{['default']}}"><uni-popup-dialog vue-id="{{('8deb4120-2')+','+('8deb4120-1')}}" mode="input" title="设置涮煮时间" value="{{editTimeValue}}" placeholder="请输入时间（秒）" data-event-opts="{{[['^confirm',[['confirmEditTime']]]]}}" onConfirm="__e" onVueInit="__l"></uni-popup-dialog></uni-popup><uni-popup vue-id="8deb4120-3" type="dialog" ref="__r" data-ref="categoryPopup" onVueInit="__l" vue-slots="{{['default']}}"><uni-popup-dialog vue-id="{{('8deb4120-4')+','+('8deb4120-3')}}" mode="input" title="添加分类" placeholder="请输入分类名称" data-event-opts="{{[['^confirm',[['confirmAddCategory']]]]}}" onConfirm="__e" onVueInit="__l"></uni-popup-dialog></uni-popup><uni-popup vue-id="8deb4120-5" type="dialog" ref="__r" data-ref="foodPopup" onVueInit="__l" vue-slots="{{['default']}}"><uni-popup-dialog vue-id="{{('8deb4120-6')+','+('8deb4120-5')}}" mode="input" title="添加食材" value="{{newFoodName}}" placeholder="请输入食材名称" data-event-opts="{{[['^confirm',[['confirmAddFoodName']]]]}}" onConfirm="__e" onVueInit="__l"></uni-popup-dialog></uni-popup><uni-popup vue-id="8deb4120-7" type="dialog" ref="__r" data-ref="timePopup" onVueInit="__l" vue-slots="{{['default']}}"><uni-popup-dialog vue-id="{{('8deb4120-8')+','+('8deb4120-7')}}" mode="input" title="设置时间" value="{{newFoodTime}}" placeholder="请输入时间（秒）" data-event-opts="{{[['^confirm',[['confirmAddFoodTime']]]]}}" onConfirm="__e" onVueInit="__l"></uni-popup-dialog></uni-popup></view>