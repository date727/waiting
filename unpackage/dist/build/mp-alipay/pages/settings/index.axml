<view class="settings-container"><block a:if="{{userInfo}}"><view class="user-info"><image class="avatar" src="{{userInfo.avatar}}" mode="aspectFit"></image><view class="info"><text class="username">{{userInfo.nickName}}</text><text class="phone">{{userInfo.mobile||'未绑定手机号'}}</text></view></view></block><view class="settings-list"><view class="setting-item"><text>通知设置</text><uni-icons vue-id="47b2531d-1" type="arrowright" size="20" color="#999" onVueInit="__l"></uni-icons></view><view class="setting-item"><text>隐私设置</text><uni-icons vue-id="47b2531d-2" type="arrowright" size="20" color="#999" onVueInit="__l"></uni-icons></view></view><view class="bottom-content"><button data-event-opts="{{[['tap',[['handleLogout',['$event']]]]]}}" class="logout-btn" onTap="__e">退出登录</button><view class="bottom-nav"><view data-event-opts="{{[['tap',[['goToHome',['$event']]]]]}}" class="nav-item" onTap="__e">Timer</view><view class="nav-item active">setting</view></view></view></view>